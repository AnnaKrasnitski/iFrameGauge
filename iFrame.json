{
	
	"name": "iFrame",
	"description": "Gauge iFrame Widget",
	"eula": "",
	"vendor": "iProsis",
	"license": "",
	"id": "com.iProsis.sample.frame",
	"newInstancePrefix": "iFrameGithub",
	"version": "1.0",
	"Access-Control-Allow-Origin": "*",
	"ignoreIntegrity":true ,
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-iprosis-sample-gauge",
			"url": "https://annakrasnitski.github.io/iFrameGauge/webcomponent.js"
		}
	],
	"properties": {
		"urlLink": {
			"type": "string",
			"description": "url link",
			"default": "https://www.sap.com/index.html"
		},
		"width": {
			"type": "number",
			"default": 300
		},
		"height": {
			"type": "number",
			"default": 300
		}
	},
	"methods": {
		"getValue": {
			"returnType": "string",
			"description": "Get the url value",
			"parameters": [],
			"body": "return this.urlLink;"
		},
		"setValue": {
			"description": "Set the url value",
			"parameters": [
				{
					"name": "value",
					"type": "string",
					"description": "The new val"
				}
			],
			"body": "this.urlLink = value;"
		}
	},
	"events": {
		"onClick": {
			"description": "Happenes when the frame is clicked"
		}
	}
}
